                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/notificationtimeid"
                        android:tag="NotificationTime"
                        app:key="NotificationTime"
                        app:value="@={Converter.toString(notificationtimeid, notificationHeader.notificationTime, notificationHeader.notificationTime)}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/tecobjnoleadingzerosid"
                        android:tag="TecObjNoLeadingZeros"
                        android:maxLength="@{40}"
                        app:key="TecObjNoLeadingZeros"
                        app:value="@={notificationHeader.tecObjNoLeadingZeros}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/technicalobjecttypedescid"
                        android:tag="TechnicalObjectTypeDesc"
