                        android:maxLength="@{2}"
                        app:key="Division"
                        app:value="@={tOFacetFilterItem.division}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
            </LinearLayout>
        </ScrollView>
