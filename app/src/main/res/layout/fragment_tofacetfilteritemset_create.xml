                    android:layout_height="wrap_content"
                    android:id="@+id/create_update_tofacetfilteritem"
                    android:orientation="vertical">

                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/objecttypedescriptionid"
                        android:tag="ObjectTypeDescription"
                        android:maxLength="@{20}"
                        app:key="ObjectTypeDescription"
                        app:value="@={tOFacetFilterItem.objectTypeDescription}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/roomid"
                        android:tag="Room"
                        android:maxLength="@{8}"
                        app:key="Room"
                        app:value="@={tOFacetFilterItem.room}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/abcindicid"
                        android:tag="AbcIndic"
                        android:maxLength="@{1}"
                        app:key="AbcIndic"
                        app:value="@={tOFacetFilterItem.abcIndic}">
