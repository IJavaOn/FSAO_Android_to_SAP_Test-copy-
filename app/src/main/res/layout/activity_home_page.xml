<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mdui.HomePage">


    <TextView
        android:id="@+id/textView9"
        android:layout_width="413dp"
        android:layout_height="64dp"
        android:background="#4351CA"
        android:gravity="center"
        android:text="Home"
        android:textColor="@color/black"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <RelativeLayout
        android:layout_width="406dp"
        android:layout_height="658dp"
        android:background="#0F124C"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="0dp" />

    <ImageButton
        android:id="@+id/imageButtoncrt"
        android:layout_width="90dp"
        android:layout_height="60dp"

        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.217"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.269"
        app:srcCompat="@android:drawable/ic_input_add"
        android:background="@drawable/edit_round"/>

    <TextView
        android:id="@+id/textViewNoti"
        android:layout_width="116dp"
        android:layout_height="63dp"
        android:gravity="center"
        android:text="Create Notification"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageButtoncrt"
        app:layout_constraintHorizontal_bias="0.309"
        app:layout_constraintStart_toStartOf="@+id/imageButtoncrt"
        app:layout_constraintTop_toBottomOf="@+id/imageButtoncrt"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/mynoti"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:layout_marginTop="4dp"

        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toEndOf="@+id/imageButtoncrt"
        app:layout_constraintTop_toTopOf="@+id/imageButtoncrt"
        app:srcCompat="@drawable/notifications"
        android:background="@drawable/edit_round"/>

    <TextView
        android:id="@+id/textView8"
        android:gravity="center"
        android:layout_width="116dp"
        android:layout_height="63dp"
        android:textColor="@color/white"
        android:text="My Notifications"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/textViewNoti"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toEndOf="@+id/textViewNoti"
        app:layout_constraintTop_toBottomOf="@+id/mynoti"
        app:layout_constraintVertical_bias="0.511" />



    <ImageButton
        android:id="@+id/work"
        android:layout_width="90dp"
        android:layout_height="60dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.476"
        app:srcCompat="@drawable/workorder"
        android:background="@drawable/edit_round"/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="118dp"
        android:layout_height="42dp"
        android:text="WorkOrder"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/work"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="@+id/work"
        app:layout_constraintTop_toBottomOf="@+id/work"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/test"
        android:layout_width="90dp"
        android:layout_height="60dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        android:background="@drawable/edit_round"
        app:layout_constraintStart_toEndOf="@+id/work"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintVertical_bias="0.126"
        app:srcCompat="@drawable/ic_account_circle_black_24dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="232dp"
        android:text="Test"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/test"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/test"
        app:layout_constraintTop_toBottomOf="@+id/test"
        app:layout_constraintVertical_bias="0.216" />

    <TextView
        android:id="@+id/textViewDate"
        android:layout_width="175dp"
        android:layout_height="60dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.153" />

    <ImageButton
        android:id="@+id/test2"
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:background="@drawable/edit_round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.715"
        app:srcCompat="@drawable/test2"
        />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="87dp"
        android:layout_height="30dp"
        android:text="Test2"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/test2"
        app:layout_constraintHorizontal_bias="0.468"
        app:layout_constraintStart_toStartOf="@+id/test2"
        app:layout_constraintTop_toBottomOf="@+id/test2"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/TestUI"
        android:layout_width="377dp"
        android:layout_height="58dp"
        android:background="@drawable/edit_round"
        app:layout_constraintBottom_toTopOf="@+id/textView21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20"
        app:srcCompat="@drawable/ic_flash_on_black_24dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:text="TestUI"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/TestUI"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/TestUI" />


</androidx.constraintlayout.widget.ConstraintLayout>